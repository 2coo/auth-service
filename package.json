{
  "name": "prisma-nexus-boilerplote",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "yarn run generate",
    "predev": "yarn run generate:prisma",
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn --transpileOnly ./src",
    "dev:migrate": "dotenv -e .env.development.local -- prisma migrate save --experimental -c && dotenv -e .env.development.local -- prisma migrate up --experimental -c",
    "prebuild": "yarn run clean && yarn run generate",
    "build": "tsc",
    "seed": "dotenv -e .env.development.local -- ts-node prisma/seed",
    "clean": "rimraf dist",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env NODE_ENV=development ts-node --transpile-only ./src/schema",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "studio": "dotenv -e .env.development.local -- prisma studio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giva9712/prisma-nexus-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/giva9712/prisma-nexus-boilerplate/issues"
  },
  "homepage": "https://github.com/giva9712/prisma-nexus-boilerplate#readme",
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "@prisma/client": "2.10.0",
    "apollo-server-express": "^2.18.2",
    "bcryptjs": "^2.4.3",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "8.2.0",
    "dotenv-flow": "^3.2.0",
    "ejs": "^3.1.5",
    "errorhandler": "^1.5.1",
    "express-session": "^1.17.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.3.3",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "lodash": "^4.17.20",
    "moment-timezone": "^0.5.31",
    "nexus-plugin-prisma": "0.23.0",
    "oauth2orize": "^1.11.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2"
  },
  "devDependencies": {
    "@prisma/cli": "2.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-ensure-login": "^0.1.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv-flow": "^3.1.0",
    "@types/errorhandler": "^1.5.0",
    "@types/ejs": "^3.0.5",
    "@types/express-session": "^1.17.0",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "^4.14.162",
    "@types/node": "12.12.54",
    "@types/oauth2orize": "^1.8.8",
    "@types/passport": "^1.0.4",
    "@types/passport-http": "^0.3.8",
    "@types/passport-http-bearer": "^1.0.36",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2-client-password": "^0.1.2",
    "@types/ws": "7.2.6",
    "cross-env": "7.0.2",
    "dotenv-cli": "4.0.0",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.50",
    "typescript": "3.9.6"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
