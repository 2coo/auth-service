{
  "name": "prisma-nexus-boilerplote",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "npm run generate:prisma",
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn --transpile-only ./src",
    "prebuild": "npm run clean && npm run generate",
    "build": "tsc",
    "seed": "ts-node prisma/seed.ts",
    "clean": "rimraf dist",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only ./src/schema",
    "start": "cross-env NODE_ENV=production node dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/giva9712/prisma-nexus-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/giva9712/prisma-nexus-boilerplate/issues"
  },
  "homepage": "https://github.com/giva9712/prisma-nexus-boilerplate#readme",
  "dependencies": {
    "@casl/ability": "^5.2.2",
    "@prisma/client": "2.16.0",
    "apollo-server-express": "2.19.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "8.2.0",
    "graphql-middleware": "^6.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "nexus": "1.0.0",
    "nexus-plugin-prisma": "0.29.0",
    "node-jose": "^2.0.0",
    "prisma": "^2.16.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "13.13.40",
    "@types/node-jose": "^1.1.5",
    "@types/prettier": "2.1.6",
    "@types/redis": "^2.8.28",
    "cross-env": "7.0.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
